{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "andreiio/blade-remix-icon": "^1.0",
    "blade-ui-kit/blade-heroicons": "^1.3",
    "calebporzio/awesome-helpers": "^2.5",
    "fruitcake/laravel-cors": "^2.0",
    "guzzlehttp/guzzle": "^7.0.1",
    "laravel/framework": "^9.0",
    "laravel/jetstream": "^2.4",
    "laravel/sanctum": "^2.11",
    "laravel/tinker": "^2.5",
    "livewire/livewire": "^2.5",
    "lorisleiva/laravel-actions": "^2.3",
    "psr/log": "^1.0",
    "sammyjo20/saloon-laravel": "^0.8.0",
    "spatie/data-transfer-object": "^3.7",
    "spatie/laravel-google-fonts": "^1.1"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.6",
    "barryvdh/laravel-ide-helper": "^2.10",
    "fakerphp/faker": "^1.9.1",
    "glhd/laravel-dumper": "^1.0",
    "laravel-shift/blueprint": "^2.0",
    "laravel/sail": "^1.0.1",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^6.1",
    "pestphp/pest": "^1.20",
    "pestphp/pest-plugin-faker": "^1.0",
    "pestphp/pest-plugin-laravel": "^1.1",
    "phpunit/phpunit": "^9.5.8",
    "spatie/laravel-ignition": "^1.0",
    "wulfheart/laravel-actions-ide-helper": "^0.3.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "app/Helpers/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "ide": [
      "@php artisan ide-helper:eloquent",
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:meta",
      "@php artisan ide-helper:models --write-mixin",
      "@php artisan ide-helper:actions"
    ],
    "pest": [
      "vendor/bin/pest"
    ],
    "fix": [
      "npx blade-formatter \"resources/**/*.blade.php\" --write"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
